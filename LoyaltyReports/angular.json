{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "LoyaltyReports": {
      "root": "",
      "sourceRoot": "lr",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/lr",
            "index": "lr/index.html",
            "main": "lr/main.ts",
            "polyfills": "lr/polyfills.ts",
            "tsConfig": "lr/tsconfig.app.json",
            "assets": [
              "lr/favicon.ico",
              "lr/assets"
            ],
            "styles": [
              "lr/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "lr/assets/css/strap-mins.scss",
              "lr/assets/css/reset.scss",
              "lr/assets/css/lr-style.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "lr/environments/environment.ts",
                  "with": "lr/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "LoyaltyReports:build",
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "LoyaltyReports:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "LoyaltyReports:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "lr/test.ts",
            "polyfills": "lr/polyfills.ts",
            "tsConfig": "lr/tsconfig.spec.json",
            "karmaConfig": "lr/karma.conf.js",
            "styles": [
              "lr/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "lr/favicon.ico",
              "lr/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "lr/tsconfig.app.json",
              "lr/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "LoyaltyReports-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "LoyaltyReports:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "LoyaltyReports:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "LoyaltyReports"
}